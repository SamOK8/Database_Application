<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="246.0" prefWidth="389.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="database_application.DatabaseAppController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ComboBox fx:id="vyberTabulky" onAction="#nacistTabulku" prefWidth="150.0"/>
      <TableView fx:id="tabulka" prefHeight="200.0" prefWidth="200.0" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="vypujcit" mnemonicParsing="false" text="Rent" />
            <Button fx:id="pridat" mnemonicParsing="false" onAction="#pridatDoTabulky" text="Add" visible="false" />
            <Button fx:id="vymazat" mnemonicParsing="false" onAction="#vymazatZTabulky" text="Delete" visible="false" />
         </children>
      </HBox>
   </children>
</VBox>
