<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="246.0" prefWidth="389.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="database_application.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label fx:id="LoginText" text="Uzivatel:" />
      <TextField fx:id="username" promptText="pepa" />
      <Label fx:id="errorLabel" textFill="#8e2020" />
        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" text="Login" />
    </children>
</VBox>
